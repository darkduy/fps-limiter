{
  "manifest_version": 3,
  "name": "FPS Limiter Pro",
  "version": "5.0.0",
  "description": "Phiên bản nâng cấp kiến trúc để sửa lỗi build.",
  "permissions": [ "storage", "scripting", "tabs" ],
  "host_permissions": [ "<all_urls>" ],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
