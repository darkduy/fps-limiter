{
  "manifest_version": 3,
  "name": "FPS Limiter Pro",
  "version": "3.0",
  "description": "Phiên bản cuối cùng: Giới hạn FPS với kiến trúc ổn định, hiệu năng cao và giao diện hiện đại.",
  "permissions": [
    "storage",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "48": "icon.png"
  }
}
